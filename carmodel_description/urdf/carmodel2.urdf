<?xml version="1.0"?>
<robot name="carmodel">

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>

  <!-- base_footprint is a fictitious link(frame) that is on the ground right below base_link origin, navigation stack depends on this frame -->

  <link name="base_footprint">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.001 0.001 0.001" />
      </geometry>
      <material name="Green" />
    </visual>

    <collision>
      <origin xyz="0 0 0.017" rpy="0 0 0" />
      <geometry>
       <box size="0.001 0.001 0.001" />
      </geometry>
    </collision>
  </link>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh
          filename="package://carmodel_description/meshes/Body3.STL" />
      </geometry>
        <origin rpy="1.5705 0 0" xyz="-0.25 0.1675 -0.0075"/>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.5 0.3 0.07"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.035"/>
    </collision>

    <inertial>
      <origin xyz="-0.00509 0 0.03461" rpy="0 0 0"/>
      <mass value="65.8282"/>
      <inertia
        ixx="63.621330" ixy="11.057206" ixz="0.021099"
        iyy="187.798863" iyz="-0.003735"
        izz="169.286082"/>
    </inertial>
  </link>

  <link name="wheel_1">
    <visual>
      <geometry>
        <mesh
          filename="package://carmodel_description/meshes/Wheel.STL" />
      </geometry>
      <origin rpy="1.5705 0 0" xyz="-0.055 0 -0.055 "/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.015" radius="0.055"/>
      </geometry>
      <origin rpy="1.5705 0 0" xyz="0 -0.0075 0"/>
    </collision>
    <inertial>
      <origin xyz="0 -0.0075 0" rpy="0 0 0"/>
      <mass value="1.0252"/>
      <inertia
        ixx="0.09065214" ixy="0.0" ixz="0.0"
        iyy="0.09065214" iyz="0.0"
        izz="0.17745989"/>
    </inertial>
  </link> 

  <link name="wheel_2">
    <visual>
      <geometry>
        <mesh
          filename="package://carmodel_description/meshes/Wheel.STL" />
      </geometry>
      <origin rpy="1.5705 0 0" xyz="-0.055 0 -0.055"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.015" radius="0.055"/>
      </geometry>
      <origin rpy="1.5705 0 0" xyz="0 -0.0075 0"/>
    </collision>
    <inertial>
      <origin xyz="0 -0.0075 0" rpy="0 0 0"/>
      <mass value="1.252"/>
      <inertia
        ixx="0.09065214" ixy="0.0" ixz="0.0"
        iyy="0.09065214" iyz="0.0"
        izz="0.17745989"/>
    </inertial>
  </link> 

  <link name="wheel_3">
    <visual>
      <geometry>
        <mesh
          filename="package://carmodel_description/meshes/Wheel.STL" />
      </geometry>
      <origin rpy="1.5705 0 0" xyz="-0.055 0 -0.055"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.015" radius="0.055"/>
      </geometry>
      <origin rpy="1.5705 0 0" xyz="0 -0.0075 0"/>
    </collision>
    <inertial>
      <origin xyz="0 -0.0075 0" rpy="0 0 0"/>
      <mass value="1.252"/>
      <inertia
        ixx="0.09065214" ixy="0.0" ixz="0.0"
        iyy="0.09065214" iyz="0.0"
        izz="0.17745989"/>
    </inertial>
  </link> 

  <link name="wheel_4">
    <visual>
      <geometry>
        <mesh
          filename="package://carmodel_description/meshes/Wheel.STL" />
      </geometry>
      <origin rpy="1.5705 0 0" xyz="-0.055 0 -0.055"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.015" radius="0.055"/>
      </geometry>
      <origin rpy="1.5705 0 0" xyz="0 -0.0075 0"/>
    </collision>
    <inertial>
      <origin xyz="0 -0.0075 0" rpy="0 0 0"/>
      <mass value="1.252"/>
      <inertia
        ixx="0.09065214" ixy="0.0" ixz="0.0"
        iyy="0.09065214" iyz="0.0"
        izz="0.17745989"/>
    </inertial>
  </link> 

  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 0.017" rpy="0 0 0" />        
    <parent link="base_footprint"/>
    <child link="base_link" />
  </joint>

  <joint name="scan_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_scan"/>
    <origin xyz="0 0 0.122" rpy="0 0 0"/>
  </joint>

  <link name="base_scan"/>

  <joint name="joint_base_wheel1" type="continuous">
    <origin rpy="0 0 0" xyz="0.13189 -0.1525 0.03018 "/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_1"/>
  </joint>

  <joint name="joint_base_wheel2" type="continuous">
    <origin rpy="0 0 0" xyz="-0.13211 -0.1525 0.03018 "/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_2"/>
  </joint>

  <joint name="joint_base_wheel3" type="continuous">
    <origin rpy="0 0 0" xyz="0.13189 0.1675 0.03018 "/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_3"/>
  </joint>

  <joint name="joint_base_wheel4" type="continuous">
    <origin rpy="0 0 0" xyz="-0.13211 0.1675 0.03018 "/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_4"/>
  </joint>

</robot>
